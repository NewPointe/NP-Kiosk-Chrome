{
    "update_url": "https://clients2.google.com/service/update2/crx",
    "name": "NP Check-in for Rock RMS",
    "version": "1.2.5",
    "manifest_version": 2,
    "icons": {
        "128": "images/icon_128.png",
        "96": "images/icon_96.png",
        "48": "images/icon_48.png"
    },
    "app": {
        "background": {
            "scripts": [
                "scripts/background.js"
            ],
            "persistent": false
        }
    },
    "permissions": [
        "webview",
        "power",
        "storage",
        "https://*/",
        "usb",
        {
            "usbDevices": [
				{
					"vendorId": 2655,
					"interfaceClass": 7
				},
				{
					"vendorId": 2655,
					"productId": 9
				},
				{
					"vendorId": 2655,
					"productId": 129
				},
				{
					"vendorId": 2655,
					"productId": 132
				},
				{
					"vendorId": 2655,
					"productId": 139
				},
				{
					"vendorId": 2655,
					"productId": 140
				},
				{
					"vendorId": 2655,
					"productId": 209
				},
				{
					"vendorId": 2655,
					"productId": 272
				}
			]
        }
    ],
    "kiosk_enabled": true,
    "webview": {
        "partitions": [
            {
                "name": "rockcheckin",
                "accessible_resources": [
                    "scripts/client-api.js"
                ]
            }
        ]
    },
    "sockets": {
        "tcp": {
            "connect": [
                "*"
            ]
        }
    },
    "storage": {
        "managed_schema": "managed-schema.json"
    },
    "commands": {
        "show-settings": {
            "suggested_key": {
                "default": "Ctrl+Shift+Period"
            },
            "description": "Shows the kiosk settings"
        }
	},
	"usbDevices": [
		[
			{
				"vendorId": "0a5f",
				"interfaceClass": 7
			},
			{
				"vendorId": "0a5f",
				"productId": "0009"
			},
			{
				"vendorId": "0a5f",
				"productId": "0081"
			},
			{
				"vendorId": "0a5f",
				"productId": "0084"
			},
			{
				"vendorId": "0a5f",
				"productId": "008b"
			},
			{
				"vendorId": "0a5f",
				"productId": "008c"
			},
			{
				"vendorId": "0a5f",
				"productId": "00d1"
			}
		]
	]
}
